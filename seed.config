# ==============================
# OpenWrt 目标配置
# ==============================
# Target System (目标架构)
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y
# 禁用构建所有配置文件 (仅构建指定设备)
# CONFIG_TARGET_ALL_PROFILES is not set

# Device Definition (设备定义)
# JDCloud RE-CS-02 (京东云路由器)
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="ipq-wifi-jdcloud_re-cs-02 ath11k-firmware-qcn9074 ath11k-firmware-ipq6018 kmod-ath11k-pci luci-app-athena-led "
# JDCloud RE-SS-01 (京东云路由器)
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01="ipq-wifi-jdcloud_re-ss-01 -kmod-ath11k-pci zram-swap "

# ==============================
# 根文件系统 & 开发者
# ==============================
# SquashFS (只读压缩文件系统)
CONFIG_TARGET_ROOTFS_SQUASHFS=y
# Per Device Rootfs (设备独立文件系统 - 禁用以免干扰固件生成)
# CONFIG_TARGET_PER_DEVICE_ROOTFS is not set
# Devel (开发者模式 - 关闭以减少干扰)
# CONFIG_DEVEL is not set
# Custom BusyBox (自定义 BusyBox)
CONFIG_BUSYBOX_CUSTOM=y
# Telnet (BusyBox Telnet 客户端)
CONFIG_BUSYBOX_CONFIG_TELNET=y

# ==============================
# 编译优化选项
# ==============================
# ccache (编译缓存)
CONFIG_CCACHE=y
# CoreMark (CPU 性能测试)

# ==============================
# 基础加密库
# ==============================
# GnuTLS
CONFIG_GNUTLS_ALPN=y
CONFIG_GNUTLS_ANON=y
CONFIG_GNUTLS_DTLS_SRTP=y
CONFIG_GNUTLS_HEARTBEAT=y
CONFIG_GNUTLS_OCSP=y
CONFIG_GNUTLS_PSK=y
# libcurl
CONFIG_LIBCURL_COOKIES=y
CONFIG_LIBCURL_CRYPTO_AUTH=y
CONFIG_LIBCURL_FILE=y
CONFIG_LIBCURL_FTP=y
CONFIG_LIBCURL_HTTP=y
CONFIG_LIBCURL_HTTP2=y
CONFIG_LIBCURL_HTTP_AUTH=y
CONFIG_LIBCURL_OPENSSL=y
CONFIG_LIBCURL_PROXY=y
CONFIG_LIBCURL_TFTP=y
CONFIG_LIBCURL_THREADED_RESOLVER=y
CONFIG_LIBCURL_TLS_SRP=y
CONFIG_LIBCURL_UNIX_SOCKETS=y
# mbedTLS
CONFIG_MBEDTLS_AESCE_C=y
CONFIG_MBEDTLS_AES_C=y
CONFIG_MBEDTLS_CMAC_C=y
CONFIG_MBEDTLS_DES_C=y
CONFIG_MBEDTLS_ECP_DP_CURVE25519_ENABLED=y
CONFIG_MBEDTLS_ECP_DP_SECP256K1_ENABLED=y
CONFIG_MBEDTLS_ECP_DP_SECP256R1_ENABLED=y
CONFIG_MBEDTLS_ECP_DP_SECP384R1_ENABLED=y
CONFIG_MBEDTLS_ECP_DP_SECP521R1_ENABLED=y
CONFIG_MBEDTLS_ENTROPY_FORCE_SHA256=y
CONFIG_MBEDTLS_GCM_C=y
CONFIG_MBEDTLS_HKDF_C=y
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_ECDSA_ENABLED=y
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_PSK_ENABLED=y
CONFIG_MBEDTLS_KEY_EXCHANGE_ECDHE_RSA_ENABLED=y
CONFIG_MBEDTLS_KEY_EXCHANGE_PSK_ENABLED=y
CONFIG_MBEDTLS_NIST_KW_C=y
CONFIG_MBEDTLS_PSA_CRYPTO_CLIENT=y
CONFIG_MBEDTLS_RSA_NO_CRT=y
CONFIG_MBEDTLS_SHA256_USE_ARMV8_A_CRYPTO_IF_PRESENT=y
CONFIG_MBEDTLS_SHA512_USE_A64_CRYPTO_IF_PRESENT=y
CONFIG_MBEDTLS_SSL_PROTO_TLS1_2=y
CONFIG_MBEDTLS_SSL_PROTO_TLS1_3=y
CONFIG_MBEDTLS_SSL_TLS1_3_COMPATIBILITY_MODE=y
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_EPHEMERAL_ENABLED=y
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_PSK_ENABLED=y
CONFIG_MBEDTLS_SSL_TLS1_3_KEY_EXCHANGE_MODE_PSK_EPHEMERAL_ENABLED=y
CONFIG_MBEDTLS_THREADING_C=y
CONFIG_MBEDTLS_THREADING_PTHREAD=y

# ==============================
# LuCI 设置
# ==============================
# 简体中文语言包
CONFIG_LUCI_LANG_zh_Hans=y
# Aurora Theme (现代化主题)
CONFIG_PACKAGE_luci-theme-aurora=y
# 兼容性库
CONFIG_PACKAGE_luci-compat=y
CONFIG_PACKAGE_luci-lua-runtime=y

# ==============================
# 核心组件
# ==============================
# APK (新一代包管理器)
CONFIG_PACKAGE_apk-mbedtls=y
# OPKG (已禁用, 使用 APK)
# CONFIG_PACKAGE_opkg is not set

# 网络工具 (Curl, Wget-SSL, TTYD)
CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_wget-ssl=y
CONFIG_PACKAGE_ttyd=y
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-i18n-ttyd-zh-cn=y

# ==============================
# 第三方插件 (Custom Packages)
# ==============================
# HomeProxy (代理管理)
CONFIG_PACKAGE_luci-app-homeproxy=y
CONFIG_PACKAGE_luci-i18n-homeproxy-zh-cn=y
# sing-box (代理内核)
CONFIG_PACKAGE_sing-box=y
# Lucky (综合工具箱)
CONFIG_PACKAGE_lucky=y
CONFIG_PACKAGE_luci-app-lucky=y
CONFIG_PACKAGE_luci-i18n-lucky-zh-cn=y
# Athena LED (雅典娜呼吸灯 - 全局启用以确保编译，亚瑟需排除)
CONFIG_PACKAGE_luci-app-athena-led=y
CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=y

# ==============================
# 系统服务
# ==============================
# DBus
CONFIG_PACKAGE_dbus=y
# SFTP 服务器
CONFIG_PACKAGE_openssh-sftp-server=y

